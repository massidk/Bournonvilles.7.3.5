                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="#">@*<img src="~/media/logoTransparant_black_sm.png"/>*@</a>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-left">
                    <li><a href="http://localhost:45336">Home</a></li>
                    <li class="dropdown">
                    <a role="button" data-toggle="dropdown" data-target="#" href="#">
                        Saloon<span class="caret"></span>
                    </a>
                    <!-- multi level dropdown -->
                    <ul class="dropdown-menu multi-level" role="menu" aria-labelledby="dropdownMenu">
                        <li><a href="~/cowboy-grill-saloon/">Cowboy Grill & Saloon</a></li>
                        <li><a href="~/menu-kort/">Menu Kort</a></li>
                        <li><a href="~/bestil-bord/">Bestil Bord</a></li>
                        <li><a href="~/information/">Information</a></li>
                        <li class="divider"></li>
                        <li class="dropdown-submenu">
