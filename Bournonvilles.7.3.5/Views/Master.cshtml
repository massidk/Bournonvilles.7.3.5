                        <ul class="dropdown-menu">
                            <li><a href="#">Cowboy Fest</a></li>
                            <li><a href="#">Show & Events</a></li>
                            <li><a href="#">Old West Popcorn</a></li>
                            <li><a href="#">Saloon 2013</a></li>
                            <li><a href="#">Diverse</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="False">News & Events<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="#">Nyhedsbrev</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="False">Om Bournonvilles<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="#">Ledige Job</a></li>
                            <li><a href="#">Forretnings Betingelser</a></li>
                            <li><a href="#">Samarbejds Partnere</a></li>
                            <li><a href="#">Referencer</a></li>
                            <li><a href="#">Links</a></li>
                        </ul>
                    </li>
                </ul>
            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>
</header>

@RenderBody()
<footer>@CurrentPage.footerText</footer>
    <!-- animated header -->
    <script src="~/Scripts/classie.js"></script>
<script src="~/Scripts/cbpAnimatedHeader.js"></script>

</body>
</html>
